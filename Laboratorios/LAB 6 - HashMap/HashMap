import java.util.HashMap;
import java.util.Scanner;

public class SumaTracudica{
    public static void main(String[] args){
        Scanner ent = new Scanner(System.in);

        HashMap<String, String> numerosEspanolAIngles = new HashMap<>();
        numerosEspanolAIngles.put("uno", "one");
        numerosEspanolAIngles.put("dos", "two");
        numerosEspanolAIngles.put("tres", "three");
        numerosEspanolAIngles.put("cuatro", "four");
        numerosEspanolAIngles.put("cinco", "five");
        numerosEspanolAIngles.put("seis", "six");
        numerosEspanolAIngles.put("siete", "seven");
        numerosEspanolAIngles.put("ocho", "eigth");
        numerosEspanolAIngles.put("nueve", "nine");
        numerosEspanolAIngles.put("cero", "zero");

        HashMap<String, Integer> numerosEspanolAEnteros = new HashMap<>();
        numerosEspanolAEnteros.put("uno", "1");
        numerosEspanolAEnteros.put("dos", "2");
        numerosEspanolAEnteros.put("tres", "3");
        numerosEspanolAEnteros.put("cuatro", "4");
        numerosEspanolAEnteros.put("cinco", "5");
        numerosEspanolAEnteros.put("seis", "6");
        numerosEspanolAEnteros.put("siete", "7");
        numerosEspanolAEnteros.put("ocho", "8");
        numerosEspanolAEnteros.put("nueve", "9");
        numerosEspanolAEnteros.put("cero", "0");

        System.out.println("Ingrese número");
        String numeroEspanol = ent.next();
        String numeroIngles = numerosEspanolAIngles.get(numeroEspanol);
        System.out.println("Tu número en inglés es: " + numeroIngles);

        String formula = ent.nextLine();

        String numero1Español = ent.next();
        String operador = ent.next();
        String numero2Español = ent.next();

        int resultado = numerosEspanolAEnteros.get(numero1Español) + numerosEspanolAEnteros.get(numero2Español);
    }
}